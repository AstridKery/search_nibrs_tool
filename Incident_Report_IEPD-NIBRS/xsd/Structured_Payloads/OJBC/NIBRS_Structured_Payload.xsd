<xsd:schema xmlns:nc="http://niem.gov/niem/niem-core/2.0" xmlns:nibrs-ext="http://ojbc.org/IEPD/Extensions/NIBRSStructuredPayload/1.0" xmlns:niem-xsd="http://niem.gov/niem/proxy/xsd/2.0" xmlns:s="http://niem.gov/niem/structures/2.0" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:j="http://niem.gov/niem/domains/jxdm/4.0" xmlns:lexslib="http://usdoj.gov/leisp/lexs/library/3.1" xmlns:fbi="http://niem.gov/niem/fbi/2.0" targetNamespace="http://ojbc.org/IEPD/Extensions/NIBRSStructuredPayload/1.0">
	<xsd:import namespace="http://niem.gov/niem/structures/2.0" schemaLocation="../../NIEM_2.0/structures/2.0/structures.xsd"/>
	<xsd:import namespace="http://niem.gov/niem/proxy/xsd/2.0" schemaLocation="../../NIEM_2.0/proxy/xsd/2.0/xsd.xsd"/>
	<xsd:import namespace="http://niem.gov/niem/niem-core/2.0" schemaLocation="../../NIEM_2.0/niem-core/2.0/niem-core.xsd"/>
	<xsd:import namespace="http://niem.gov/niem/domains/jxdm/4.0" schemaLocation="../../NIEM_2.0/domains/jxdm/4.0/jxdm.xsd"/>
	<xsd:import namespace="http://usdoj.gov/leisp/lexs/library/3.1" schemaLocation="../../lexs/library/3.1/library.xsd"/>
	<xsd:import namespace="http://niem.gov/niem/fbi/2.0" schemaLocation="../../NIEM_2.0/fbi/2.0/fbi.xsd"/>
	<xsd:annotation>
		<xsd:documentation>Structured payload for NIBRS Report</xsd:documentation>
	</xsd:annotation>
	<xsd:complexType name="IncidentReportType">
		<xsd:annotation>
			<xsd:documentation>A data type for a NIBRS report.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="s:ComplexObjectType">
				<xsd:sequence>
					<xsd:element ref="nibrs-ext:Property"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="PropertyType">
		<xsd:annotation>
			<xsd:documentation>A data type for an item that is owned, possessed, or otherwise acquired by an entity.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="nc:PropertyType">
				<xsd:sequence>
					<xsd:element ref="nc:ItemCategory"/>
					<xsd:element ref="j:IncidentStolenVehicleQuantity"/>
					<xsd:element ref="j:IncidentRecoveredVehicleQuantity"/>
					<xsd:element ref="lexslib:SameAsDigestReference"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:element name="IncidentReport" type="nibrs-ext:IncidentReportType">
		<xsd:annotation>
			<xsd:documentation>A data type for an Incident Reportt.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Property" type="nibrs-ext:PropertyType">
		<xsd:annotation>
			<xsd:documentation>An item that is owned, possessed, or otherwise acquired by an entity.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
</xsd:schema>
