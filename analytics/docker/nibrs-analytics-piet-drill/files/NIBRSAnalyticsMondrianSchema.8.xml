<?xml version="1.0" encoding="UTF-8"?>
<Schema name="nibrs_analytics" caption="NIBRS Analytics" description="NIBRS Analytics">
  <Dimension name="Hour">
    <Hierarchy hasAll="true" allMemberName="All Hours" name="Hour" caption="Hour of Day" primaryKey="HourTypeID">
      <Table name="HourType"/>
      <Level name="HourTypeCategory" column="HourTypeCategory" ordinalColumn="HourTypeCategorySort" caption="AM/PM"/>
      <Level name="HourTypeDetailCategory" column="HourTypeDetailCategory" ordinalColumn="HourTypeDetailCategorySort" caption="Part of Day"/>
      <Level name="HourTypeDescription" column="HourTypeDescription" ordinalColumn="HourTypeID" caption="Hour"/>
    </Hierarchy>
  </Dimension>
  <Dimension name="Agency">
    <Hierarchy primaryKeyTable="Agency" hasAll="true" allMemberName="All Agencies" name="Agency" caption="Agency" primaryKey="AgencyORI">
      <Join leftKey="AgencyTypeID" rightKey="AgencyTypeID">
        <Table name="AgencyType"/>
        <Table name="Agency"/>
      </Join>
      <Level name="AgencyTypeDescription" column="StateDescription" table="AgencyType" caption="Agency Type"/>
      <Level name="AgencyName" column="AgencyName" table="Agency" caption="Agency"/>
    </Hierarchy>
    <Hierarchy primaryKeyTable="Agency" hasAll="true" allMemberName="All Agencies" name="AgencyORI" caption="Agency ORI" primaryKey="AgencyORI">
      <Join leftKey="AgencyTypeID" rightKey="AgencyTypeID">
        <Table name="AgencyType"/>
        <Table name="Agency"/>
      </Join>
      <Level name="AgencyTypeDescription" column="StateDescription" table="AgencyType" caption="Agency Type"/>
      <Level name="AgencyORI" column="AgencyORI" table="Agency" caption="Agency ORI"/>
    </Hierarchy>
  </Dimension>
  <Dimension name="State">
    <Hierarchy hasAll="true" allMemberName="All States" name="State" caption="State" primaryKey="StateID">
      <Table name="State"/>
      <Level name="StateName" column="StateName" caption="State"/>
    </Hierarchy>
  </Dimension>
  <Dimension name="AdditionalJustifiableHomicideCircumstances" caption="Justifiable Homicide Circumstances">
